{
  "name": "jolc",
  "version": "2.0.3",
  "scripts": {
    "start": "python3 -m uvicorn api.main:app --host=0.0.0.0 --port=${PORT:-5000}",
    "dev:client": "cd client && vite",
    "dev:api": "cd api && uvicorn main:app --reload --port 300",
    "build": "cd client && vite build",
    "serve": "cd client && vite preview"
  },
  "dependencies": {
    "ace-builds": "1.13.0",
    "axios": "1.1.3",
    "graphviz-react": "1.2.5",
    "react": "17.0.0",
    "react-ace": "10.1.0",
    "react-dom": "17.0.0",
    "react-redux": "8.0.5",
    "react-split-grid": "1.0.4",
    "redux-devtools-extension": "2.13.9",
    "wouter": "2.8.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react-refresh": "1.3.6",
    "eslint-plugin-react": "7.31.10",
    "vite": "3.2.3"
  },
  "license": "MIT"
}
